/* pages/align-self/align-self.wxss */
.box{
  background-color: blueviolet;
  display: flex;
}

.card{
  background-color:brown;
  width: 100px;
  height: 100px;
  margin: 10px;
}

.start{
  align-self: flex-start;
}
.end{
  align-self: flex-end;
}
.auto{
  align-self: auto;
}

.center{
  align-self: center;
}

.baseline{
  align-self: baseline;
}
.stretch{
  align-self: stretch;
}